{
    "name": "larabros/dockstead",
    "description": "A Docker environment for web \"artisans\"",
    "keywords": [
        "larabros",
        "dockstead",
        "docker"
    ],
    "homepage": "https://github.com/larabros/dockstead",
    "license": "MIT",
    "authors": [
        {
            "name": "Hassan Khan",
            "email": "contact@hassankhan.me",
            "homepage": "http://hassankhan.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "symfony/console": "~2.3|~3.0",
        "symfony/process": "~2.3|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Larabros\\Dockstead\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larabros\\Dockstead\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "bin": [
        "dockstead"
    ]
}
